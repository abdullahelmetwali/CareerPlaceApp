{
  "expo": {
    "name": "career-place",
    "slug": "career-place",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/career-place.png",
      "resizeMode": "contain",
      "backgroundColor": "#D2FF1F"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#D2FF1F",
          "image": "./assets/images/career-place.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "adaptiveIcon" : {
        "foregroundImage": "./assets/images/career-place.png",
        "backgroundColor": "#D2FF1F"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/career-place.png",
        "backgroundColor": "#D2FF1F"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
